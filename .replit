modules = ["python-3.11", "web"]
[agent]
expertMode = true
integrations = ["python_gemini:1.0.0", "github:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["openssl", "postgresql"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "APK Editor Web Server"

[[workflows.workflow]]
name = "APK Editor Web Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python app.py --web"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 39969
externalPort = 3000

[[ports]]
localPort = 44181
externalPort = 80

[deployment]
deploymentTarget = "autoscale"
run = ["python", "app.py", "--web"]
